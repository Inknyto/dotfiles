mkcd='function _mkcd() { mkdir -p "$1" && cd "$1"; }; _mkcd'
cpcd='function _cpcd() { cp "$1" "$2" && cd "$2"; }; _cpcd'
mvcd='function _mvcd() { mv "$1" "$2" && cd "$2"; }; _mvcd'
mkmv='function _mkmv() { mkdir $2 && mv "$1" "$2" && cd "$2"; }; _mkmv'
mkcp='function _mkcp() { mkdir $2 && cp "$1" "$2" && cd "$2"; }; _mkcp'
